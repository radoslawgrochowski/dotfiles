[user]
  name = Rados≈Çaw Grochowski
[core]
  editor = vi
  excludesFile = ~/.gitignore
[diff]
	tool = vimdiff
	colorMoved = zebra
[merge]
	tool = vimdiff
[mergetool "vimdiff"]
	path = nvim
[url "git@github.com:"]
	insteadOf = https://github.com/
[push]
	default = current
	autoSetupRemote = true
[fetch]
  prune = true
[include]
  path = ~/.gitconfig.local
