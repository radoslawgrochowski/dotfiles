{
  "$schema": "https://opencode.ai/config.json",
  "autoupdate": false,
  "theme": "tokyonight",
  "share": "disabled",
  "keybinds": {
    "messages_half_page_up": "ctrl+u",
    "messages_half_page_down": "ctrl+d"
  },
  "provider": {
    "google-vertex": {
      "models": {
        "gemini-2.5-flash": {},
        "gemini-2.5-pro": {}
      },
      "options": {
        "project": "{env:GOOGLE_CLOUD_PROJECT}",
        "location": "{env:GOOGLE_CLOUD_LOCATION}"
      }
    }
  }
}
